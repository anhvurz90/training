<nodeTypes xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0">
  

  <nodeType name="frm:match" isMixin="false" hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>nt:base</supertype>
    </supertypes>
    <propertyDefinitions>
      <propertyDefinition name="frm:equipeRecevant" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY"
        protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>        
      <propertyDefinition name="frm:equipeInvitee" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY"
        protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="frm:amical" requiredType="Boolean" autoCreated="false" mandatory="false" onParentVersion="COPY"
        protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="frm:rencontre" requiredType="Date" autoCreated="false" mandatory="false" onParentVersion="COPY"
        protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="frm:sport" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY"
        protected="false" multiple="false">
        <valueConstraints>
          <valueConstraint>foot</valueConstraint>
          <valueConstraint>baseball</valueConstraint>
          <valueConstraint>rugby</valueConstraint>
        </valueConstraints>        
      </propertyDefinition>      
    </propertyDefinitions>
    <childNodeDefinitions>
      <childNodeDefinition name="*" defaultPrimaryType="nt:file" autoCreated="false" mandatory="false"
        onParentVersion="VERSION" protected="false" sameNameSiblings="true">
        <requiredPrimaryTypes>
          <requiredPrimaryType>nt:file</requiredPrimaryType>
        </requiredPrimaryTypes>
      </childNodeDefinition>
      <childNodeDefinition name="exo:image" defaultPrimaryType="nt:resource" autoCreated="false" mandatory="false"
        onParentVersion="VERSION" protected="false" sameNameSiblings="true">
        <requiredPrimaryTypes>
          <requiredPrimaryType>nt:resource</requiredPrimaryType>
        </requiredPrimaryTypes>
      </childNodeDefinition>
    </childNodeDefinitions>
  </nodeType>   

  <nodeType name="frm:matchfolder" isMixin="false" hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>nt:base</supertype>
    </supertypes>
    <childNodeDefinitions>
        <childNodeDefinition name="*" defaultPrimaryType="frm:match" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" sameNameSiblings="true">
        <requiredPrimaryTypes>
          <requiredPrimaryType>frm:match</requiredPrimaryType>
        </requiredPrimaryTypes>
      </childNodeDefinition>
    </childNodeDefinitions>
  </nodeType>  
      
</nodeTypes>
