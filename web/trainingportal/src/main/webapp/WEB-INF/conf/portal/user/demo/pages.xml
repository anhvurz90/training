<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page>
    <page-id>user::demo::rss</page-id>
    <owner-type>user</owner-type>
    <owner-id>demo</owner-id>
    <name>rss</name>
    <title>RSS</title>
    <access-permissions>demo</access-permissions>
    <edit-permission>demo</edit-permission>
    <application>
      <instance-id>user#demo:/content/RSSReaderPortlet/content</instance-id>
      <title>RSS Reader</title>
      <show-info-bar>false</show-info-bar>
      <show-application-state>true</show-application-state>
    </application>
  </page>
  <page>
    <page-id>user::demo::exercice3</page-id>
    <owner-type>user</owner-type>
    <owner-id>demo</owner-id>
    <name>exercice3</name>
    <title>Exercice 3</title>
    <access-permissions>demo</access-permissions>
    <edit-permission>demo</edit-permission>

    <container id="TopPane" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
      <application>
        <instance-id>user#demo:/content/RSSReaderPortlet/rssdemo</instance-id>
        <title>RSS Reader</title>
        <show-info-bar>true</show-info-bar>
        <show-application-state>true</show-application-state>
      </application>
      <application>
        <instance-id>user#demo:/forum/ForumPortlet/forumdemo</instance-id>
        <title>FORUM</title>
        <show-info-bar>true</show-info-bar>
        <show-application-state>true</show-application-state>
      </application>
    </container>
    <container id="BottomPane" template="system:/groovy/portal/webui/container/UITabContainer.gtmpl">
      <application>
        <instance-id>user#demo:/web/IFramePortlet/google</instance-id>
        <title>Google</title>
        <show-info-bar>true</show-info-bar>
        <show-application-state>true</show-application-state>
        <show-application-mode>true</show-application-mode>
      </application>
      <application>
        <instance-id>user#demo:/web/IFramePortlet/yahoo</instance-id>
        <title>Yahoo</title>
        <show-info-bar>true</show-info-bar>
        <show-application-state>true</show-application-state>
        <show-application-mode>true</show-application-mode>
      </application>
    </container>

  </page>
</page-set>
