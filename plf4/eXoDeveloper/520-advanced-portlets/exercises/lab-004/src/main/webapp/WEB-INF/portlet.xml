<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<description xml:lang="EN">Caching Portlet 004</description>
		<portlet-name>CachingPortlet-004</portlet-name>
		<portlet-class>
      org.exoplatform.training.portlet.caching.CachingPortlet
    </portlet-class>
		<expiration-cache>10</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Cached Portlet-004</title>
      <keywords>training, demo, simple</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>message</name>
				<value>Hello World!</value>
				<read-only>false</read-only>
			</preference>
		</portlet-preferences>
	</portlet>


	<portlet>
		<description xml:lang="EN">No Caching Portlet 004</description>
		<portlet-name>NoCachingPortlet-004</portlet-name>
		<portlet-class>
      org.exoplatform.training.portlet.caching.CachingPortlet
    </portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Not Cached Portlet-004</title>
      <keywords>training, demo, simple</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>message</name>
				<value>Hello World!</value>
				<read-only>false</read-only>
			</preference>
		</portlet-preferences>
	</portlet>



</portlet-app>
