<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<description xml:lang="EN">Public Parameter Consumer 003</description>
		<portlet-name>PublicParamterConsumer-003</portlet-name>
		<portlet-class>
      org.exoplatform.training.portlet.parameters.ConsumerPortlet
    </portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Public Parameter Consumer-003</title>
      <keywords>training, demo, simple</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>message</name>
				<value>Hello World!</value>
				<read-only>false</read-only>
			</preference>
		</portlet-preferences>		
		
		<supported-public-render-parameter>country</supported-public-render-parameter>
	    
	</portlet>


	<portlet>
		<description xml:lang="EN">Public Parameter Form 003</description>
		<portlet-name>PublicParameterForm-003</portlet-name>
		<portlet-class>
        org.exoplatform.training.portlet.parameters.FormPortlet
        </portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Public Parameter Form-003</title>
      <keywords>training, demo, simple</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>message</name>
				<value>Hello World!</value>
				<read-only>false</read-only>
			</preference>
		</portlet-preferences>		
		
		<supported-public-render-parameter>country</supported-public-render-parameter>
	    
	</portlet>





  <public-render-parameter>
    <identifier>country</identifier>
    <name>country</name>
  </public-render-parameter>  



</portlet-app>
